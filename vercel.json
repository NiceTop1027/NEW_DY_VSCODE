{
  "version": 2,
  "name": "dy-vscode",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/terminal",
      "dest": "server.js"
    },
    {
      "src": "/debug",
      "dest": "server.js"
    },
    {
      "src": "/lsp",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 60
    }
  }
}

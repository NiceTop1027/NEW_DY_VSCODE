  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "build": "webpack --config webpack.config.js"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
